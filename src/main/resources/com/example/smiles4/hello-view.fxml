<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" prefHeight="316.0" prefWidth="544.0" spacing="20.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/17.0.2-ea" fx:controller="com.example.smiles4.Controller">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <Pane fx:id="pane" prefHeight="295.0" prefWidth="537.0">

      <children>
         <Button alignment="TOP_RIGHT" layoutX="14.0" layoutY="256.0" mnemonicParsing="false" text="Добрый" onAction="#clickGood" />
         <Button layoutX="82.0" layoutY="256.0" mnemonicParsing="false" text="Злой" onAction="#clickBad"/>
         <Button layoutX="142.0" layoutY="256.0" mnemonicParsing="false" text="Нейтральный" onAction="#clickNeutral"/>
      </children>
   </Pane>

    <Label fx:id="welcomeText" />
</VBox>
